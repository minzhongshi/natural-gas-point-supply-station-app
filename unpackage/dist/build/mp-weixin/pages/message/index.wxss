page{font-family:montserrat;background-image:linear-gradient(125deg,#CB9FFE,#5894F7,#ABCDFA,#74A3F6,#CB9FFE);background-size:400%;animation:bganimation 15s infinite}.consume_view{display:flex;justify-content:center;width:90%;height:190rpx;position:relative;margin:50rpx auto;background-color:#fff;color:#312c34;box-sizing:border-box;border-radius:10rpx}.consume_view .desc{position:absolute;left:80rpx;top:20rpx}.consume_view .desc view{padding:10rpx 0;display:flex;align-items:center}.consume_view .desc .left{font-size:50rpx}.consume_view .right{position:absolute;right:30rpx;bottom:0;font-size:30rpx;font-weight:400}.small_text{font-size:30rpx}.font-s-34{font-size:34rpx}.font-s-36{font-size:36rpx}.font-s-38{font-size:38rpx}.font-s-40{font-size:40rpx}.middle_text{font-size:36rpx}.text_wide_900{font-weight:900}.text_wide_600{font-weight:600}li{list-style-type:none}.consume_tip{display:inline-block;text-align:center;width:188rpx;height:30rpx;line-height:30rpx;position:absolute;top:36rpx;right:-44rpx;z-index:2;overflow:hidden;transform:rotate(45deg);-ms-transform:rotate(45deg);-moz-transform:rotate(45deg);-webkit-transform:rotate(45deg);-o-transform:rotate(45deg);border:1px dashed;box-shadow:0 0 0 3px #e43d33,0 21px 5px -18px rgba(0,0,0,.6);background:#E43D33;font-size:26rpx;color:#fff}.wrap{width:100%;height:100%;position:absolute;top:-12rpx;left:12rpx;overflow:hidden}.wrap:before{content:"";display:block;border-radius:8px 8px 0 0;width:80rpx;height:14rpx;position:absolute;right:68rpx;top:-1px;background:#4D6530}.wrap:after{content:"";display:block;border-radius:0 8px 8px 0;width:14rpx;height:80rpx;position:absolute;right:-1px;top:66rpx;background:#4D6530}@keyframes bganimation{0%{background-position:0% 50%}50%{background-position:100% 50%}to{background-position:0% 50%}}
